            /* input style */

            .input-style {
                padding: 10px;
                border: 2px solid #ccc;
                border-radius: 5px;
                font-size: 16px;
                color: #555;
                outline: none;
            }

            .input-style:focus {
                border-color: #007bff;
                box-shadow: 0 0 0 0.2rem rgba(0, 123, 255, 0.25);
            }

            /* --------------------------- */

            /* Button style */
            .button:hover::before {
                transform: scaleX(1);
            }

            .button-content {
                position: relative;
                z-index: 1;
            }

            .button::before {
                content: "";
                position: absolute;
                top: 0;
                left: 0;
                transform: scaleX(0);
                transform-origin: 0 50%;
                width: 100%;
                height: inherit;
                border-radius: inherit;
                background: linear-gradient(82.3deg,
                        rgb(210, 71, 71) 10.8%,
                        rgb(221, 19, 19) 94.3%);
                transition: all 0.475s;
            }

            /* --------------------------- */
            /* Input File style */
            .custum-file-upload {
                height: 11vh;
                width: 7.1vw;
                display: flex;
                flex-direction: column;
                align-items: space-between;
                gap: 0px;
                cursor: pointer;
                align-items: center;
                justify-content: center;
                border: 2px dashed #cacaca;
                background-color: rgba(255, 255, 255, 1);
                padding: -0.5rem;
                border-radius: 10px;
                box-shadow: 0px 48px 35px -48px rgba(0, 0, 0, 0.1);
            }

            .custum-file-upload .icon {
                display: flex;
                align-items: center;
                justify-content: center;
            }

            .custum-file-upload .icon svg {
                height: 5vh;
                fill: rgba(75, 85, 99, 1);
            }

            .custum-file-upload .text {
                display: flex;
                align-items: center;
                justify-content: center;
            }

            .custum-file-upload .text span {
                font-size: 0.7rem;
                font-weight: lighter;
                color: rgba(75, 85, 99, 1);
            }

            .custum-file-upload input {
                display: none;
            }

            /* ------------------------------- */
            label {
                font-weight: bold;
            }

            .inp {
                height: 5vh;
                width: 25vw;
            }

            input {
                height: 3vh;
            }

            .inBox {
                margin: 0.5rem;
            }

            .sidepanel a:hover {
                color: black;
                text-decoration: none;
            }

            a {
                text-decoration: none;
                color: #15161D;
            }

            .options {
