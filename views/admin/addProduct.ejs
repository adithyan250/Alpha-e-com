<script>
    function previewBeforeUpload(id) {
        document.querySelector("#" + id).addEventListener("change", function (e) {
            if (e.target.files.length == 0) {
                return;
            }
            let file = e.target.files[0];
            let url = URL.createObjectURL(file);
            document.querySelector("#" + id + "-preview-img").src = url;
        })
    }

    previewBeforeUpload("file-1");
    previewBeforeUpload("file-2");
    previewBeforeUpload("file-3");
    previewBeforeUpload("file-4");
    previewBeforeUpload("file-5");
</script>

<script>
    function _(element) {
        return document.getElementById(element);
    }

    function fetch_data(category, subcategory) {
        fetch('/admin/admin_panel/products/add_product_get_data?cat=' + category.value + '').then(function (response) {
            return response.json();
        }).then(function (responseData) {
            console.log(responseData);
            var html = '';

            for (var count = 0; count < responseData.length; count++) {
                html += '<option value="' + responseData[count].subCategory_id + '">' + responseData[count].subCategory_name + '</option>';
            }

            subcategory.innerHTML = html;
        });
    }

    _('category').onchange = function () {

        fetch_data(_('category'), _('subcategory'));

    };
</script>

